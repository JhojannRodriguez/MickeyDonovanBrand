    %nav{:class => "navigator navigator-static purple-background" , :id => "navigator"}
      %a{:href => "#", :class => "navigator__containerlogo"}
        %img{:src => "IMAGES/Design/Logo Mickey Donovan.png" , :class => "navigator__logo" , :alt => "Mickey Donovan Logo"}
      %button{:type => "button", :class => "reponsive-button" , :id => "responsiveButton"}
        %div{:class => "iconMenu"}
      %ul{:class => "navigator__containerbottons navigator-hidden"}
